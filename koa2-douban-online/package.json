    "koa-static": "^5.0.0",
    "koa-views": "^6.1.4",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.9",
    "pug": "^2.0.3",
    "puppeteer": "^1.9.0",
    "ramda": "^0.25.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
